pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('CheckOut'){
            steps{
                git url:"https://github.com/sh1vam31/devopsFinal.git",branch:'main'
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
            }
        }
    }
}